@keyframes crescendo 
  transition-delay: 2s
  0% 
  background-size:28px
  width:28px
  height:28px
  100% 
  background-size:50px 
  width:50px
  height:50px
  

#trackpath 
  stroke-dasharray: 2000
  animation: initial
  transition: opacity 2s ease-in-out !important
  transition: stroke-dashoffset 5s ease-in-out !important
  z-index: 9
  
#trackpathh 
  stroke-dasharray: 2000
  animation: intial
  transition: opacity 3s ease-in-out !important
  transition: stroke-dashoffset 7.5s ease-in-out !important
  z-index: 9
  
#trent 
  z-index: 9

 
@keyframes pop
  0%
  stroke-dashoffset: 2000
  100%
  stroke-dashoffset: 0
  
