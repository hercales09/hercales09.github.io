  text-align: center
  top: 0
  left: 0
  width: 100%
  height: 0
  z-index: 2000
.connection > .alert
  border-top: 0
  border-top-left-radius: 0
  border-top-right-radius: 0
  display: inline-block

#target-box
  width:50px
  height: 50px
  opacity:20%
  position: absolute
  top: 0%
  left:10%
  transform: translate(0%, 0%)
  transform-origin: 0% 0%;
  border: 5px dotted #fff

#target
  background url(https://ik.imagekit.io/ctmfcct2/balltrans_aG1oGmtE10.png?ik-sdk-version=javascript-1.4.3&updatedAt=1665248818535) no-repeat
  background-size:70px
  width: 90px
  height: 90px
  position: absolute
  top: 50%
  left: 50%;
  transform: translate(-50%, -50%)
  border-radius: 50%
  transform-origin:50% 50%
  transition: all 1.2s ease-in-out
  animation:initial
  transform: scale(2)
  filter:drop-shadow(4px 4px 5px #000000c4)
  
  
